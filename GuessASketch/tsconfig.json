        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react"
    },
    "files": [
        "typings/globals/es6-shim/index.d.ts",
        "typings/globals/meteor/index.d.ts",
        "typings/index.d.ts",
